  public static void sortArray(ArrayList<Integer> arr, int n) {
        // Write your code here.
        for(int i=0;i<n;i++){
            for(int j=i+1;j<n;j++){
                if(arr.get(i)>arr.get(j)){
                    int temp=arr.get(i);
                   arr.set(i,arr.get(j));
                    arr.set(j,temp);
                }
            }
        }
    }
